{
  "version": "0.2.0",
  "configurations": [
    {
      "command": "npm start",
      "name": "Run npm start",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "name": "Attach",
      "port": 9229,
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "type": "minecraft-js",
      "request": "attach",
      "name": "/script debugger connect localhost 1000",
      "mode": "listen",
      "localRoot": "${workspaceFolder}/",
      "port": 1000
    }
  ]
}